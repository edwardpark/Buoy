<%= form_for @event.id.venue do |f| %>
event_id: <%= @event.id %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>

<%= f.check_box :_destroy %>
<%= f.label :_destroy, "Remove" %>
<%= button_to "delete", @event, :method => :delete, data: {confirm: "Are you sure you want to delete this event?"}, class: "btn btn-lg btn-danger" %>

<%= f.submit, class: "btn btn-lg btn-primary" %>